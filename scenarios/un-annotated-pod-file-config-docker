# Settings

datadog-values.yaml
  logs:
    enabled: true
    containerCollectAll: false
  confd:
    "bash.yaml": |-
      ad_identifiers:
       - bash
      logs:
        service: testy
        source: testy

---

k run bash --image=bash \
	--command -- bash -c 'while true; do date; sleep 1; done'

